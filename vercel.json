{
  "version": 2,
  "public": true,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "ui/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "ui/$1",
      "check": true
    },
    {
      "src": "/(.*)",
      "status": 200,
      "dest": "ui/index.html"
    }
  ]
}

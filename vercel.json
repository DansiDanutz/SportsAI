{
  "version": 2,
  "public": true,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "echo \"Using root build script (installs + builds frontend via --prefix)\"",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

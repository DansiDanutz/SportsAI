{
  "version": 2,
  "public": true,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "ui/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

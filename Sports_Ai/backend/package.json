{
  "name": "sportsai-backend",
  "version": "5.0.0",
  "description": "SportsAI Platform Backend - Arbitrage Priority",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "playwright install chromium",
    "dev": "ts-node-dev --respawn --project tsconfig.json src/index.ts",
    "build": "prisma generate && prisma db push && tsc",
    "start": "node dist/index.js",
    "vercel-build": "prisma generate && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:create-user": "tsx create-user.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^9.2.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^7.7.3",
    "@fastify/static": "^6.12.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-fastify": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^10.0.0",
    "@prisma/client": "^6.19.1",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.8",
    "@swc/helpers": "^0.5.18",
    "@types/bcryptjs": "^2.4.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.6",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "dotenv": "^17.2.3",
    "ioredis": "^5.3.0",
    "kafkajs": "^2.2.0",
    "otplib": "^13.0.2",
    "passport-jwt": "^4.0.0",
    "playwright": "^1.57.0",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.0",
    "tsx": "^4.21.0",
    "uuid": "^13.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cron": "^2.0.1",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prisma": "^6.19.1",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}

# SportsAI Backend Environment Variables
# Copy this to .env and fill in your values

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/sportsai

# JWT Auth
JWT_SECRET=your-jwt-secret-here

# OpenRouter (AI predictions, sentiment analysis)
OPENROUTER_API_KEY=sk-or-v1-your-key-here

# The Odds API (live odds - https://the-odds-api.com)
# Free: 500 credits/mo at https://the-odds-api.com
THE_ODDS_API_KEY=your-odds-api-key

# Port
PORT=3001
